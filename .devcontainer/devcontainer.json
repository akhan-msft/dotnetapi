{
  "name": ".NET 9 & SQL Server",
  "dockerComposeFile": "docker-compose.yml",
  "service": "webapi", // Changed to webapi service
  "workspaceFolder": "/workspace",
  "forwardPorts": [5012, 1433],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "ms-dotnettools.csdevkit",
        "ms-azuretools.vscode-docker",
        "ms-mssql.mssql" // Added SQL Server extension for testing and executing SQL queries
      ]
    }
  },
  "remoteUser": "vscode",
  "features": {
    "docker-in-docker": {
      "version": "latest"
    }
  }
}